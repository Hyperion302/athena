{
  "name": "athena-backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "start": "node -r dotenv/config ./dist/index.js",
    "start:dev": "concurrently -n NODE,TSC \"nodemon -r dotenv/config ./dist/index.js\" \"tsc -w\"",
    "start:proxy": "cloud_sql_proxy -instances=direct-discord-democracy:us-central1:drkt-instance=tcp:3306 -credential_file=credentials.json",
    "start:proxy:docker": "docker run -d -v $(pwd)/credentials.json:/credentials.json -p 127.0.0.1:3306:3306 gcr.io/cloudsql-docker/gce-proxy:1.16 /cloud_sql_proxy -instances=direct-discord-democracy:us-central1:drkt-instance=tcp:0.0.0.0:3306 -credential_file=/credentials.json"
  },
  "author": "Joseph Shetaye",
  "license": "ISC",
  "devDependencies": {
    "@types/concurrently": "^6",
    "@types/cors": "^2",
    "@types/node": "^14.17.1",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "concurrently": "^6.2.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsdoc": "^35.1.3",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "nodemon": "^2.0.7",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.1.6",
    "typescript": "^3.9.9"
  },
  "dependencies": {
    "@types/btoa": "^1.2.3",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.12",
    "@types/flake-idgen": "^0.1.30",
    "@types/qs": "^6.9.6",
    "@types/ws": "^7.4.4",
    "array-flat-polyfill": "^1.0.1",
    "athena-common": "*",
    "axios": "^0.20.0",
    "btoa": "^1.2.1",
    "chevrotain": "^7.1.2",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "discord.js": "^12.5.3",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "flake-idgen": "^1.4.0",
    "knex": "^0.21.19",
    "mysql2": "^2.2.5",
    "qs": "^6.10.1",
    "sqlite3": "^5.0.2",
    "winston": "^3.3.3"
  }
}
